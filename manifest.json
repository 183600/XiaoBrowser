{
  "manifest_version": 3,
  "name": "小浏览器",
  "version": "1.0",
  "description": "弹出弹窗或者显示侧栏，在里面显示一个浏览器，有一排访问网页的按钮，可以从书签里添加，基于chrome-extensions-samples-main修改，使用MIT许可开源",
"background": {
    "service_worker": "background/service-worker.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "src/icons/icon.png"
  },
 "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["js/content.js"]
   }
 ],
"options_page": "options.html",
"icons": {"48": "src/icons/icon48.png",
          "128": "src/icons/icon192.png" },
  "permissions": [
      "tabs",
"storage",
"activeTab"
  ]
}

