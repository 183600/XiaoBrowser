{
    "manifest_version": 3,
    "name": "xsy_ext",
    "version": "1.0.0",
    "description": "vue3_vite_chrome_extension",
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "./src/popup/index.html"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": [
            "content_scripts.js"
        ],
        "run_at": "document_end"
    }],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}